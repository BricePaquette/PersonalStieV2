<!DOCTYPE html>
<html lang="zxx">
<head>

  <!-- ** Basic Page Needs ** -->
  <meta charset="utf-8">
  <title>Brice Paquette</title>

  <!-- ** Mobile Specific Metas ** -->
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="description" content="Hi, my name is Brice, and this is my website. Enjoy :)">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">
  <meta name="author" content="Themefisher">
  <meta name="generator" content="Themefisher Html5 Portfolio Template v1.0">

  <!-- Essential Stylesheets -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Libre+Baskerville:400,400i|Open+Sans:400,600,700,800">
  <link rel="stylesheet" href="~/plugins/bootstrap/bootstrap.min.css">
  <link rel="stylesheet" href="~/plugins/animate.css">
  <link rel="stylesheet" href="~/plugins/slick/slick.css">
  <link rel="stylesheet" href="~/plugins/slick/slick-theme.css">
  <link rel="stylesheet" href="~/plugins/themefisher-fonts/css/themefisher-fonts.min.css">  

  <!-- Main Stylesheet -->
  <link rel="stylesheet" href="~/css/style.css">

  <!-- Personalized Styles -->
  <link rel="stylesheet" href="~/css/main.css" />
  
  <!-- Theme Stylesheet -->
  <link rel="stylesheet" href="#" id="color-changer">
  
  <!--Favicon-->
  <link rel="icon" href="#" type="image/x-icon" id="favicon">
</head>
<body class="dark" id="theme"><!-- set class="dark" on body tag for DARK-THEME -->

  <div class="preloader">
    <div class="loading-mask"></div>
    <div class="loading-mask"></div>
    <div class="loading-mask"></div>
    <div class="loading-mask"></div>
    <div class="loading-mask"></div>
  </div>

  <div class="preview-wrapper">
    <div class="switcher-head">
      <span>Style Switcher</span>
      <div class="switcher-trigger tf-tools"></div>
    </div>

    <div class="switcher-body">
      <h4>Choose Color:</h4>
      <ul class="color-options list-none">
        <li class="c0" data-color="red" title="Red" onclick="setColor('red')">Red</li>
        <li class="c1" data-color="blue" title="Blue" onclick="setColor('blue')">Blue</li>
        <li class="c2" data-color="green" title="Green" onclick="setColor('green')">Green</li>
        <li class="c3" data-color="yellow" title="Yellow" onclick="setColor('yellow')">Yellow</li>
      </ul>
      
    </div>
  </div>
  

  <main class="site-wrapper">

  @RenderBody()
    <!-- /.page-nav -->
    <footer>
        <hr class="copy-line">
        <p class="text-center">Copyright &copy; 2021, Designed &amp; Developed by <a href="https://themefisher.com/">Themefisher</a>.</p>
    </footer>

    <!-- /.container -->
    
    </main> <!-- /.site-wrapper -->
